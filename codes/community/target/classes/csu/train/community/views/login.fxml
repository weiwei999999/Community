<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="csu.train.community.views.LoginController">
   <children>
      <Label layoutX="106.0" layoutY="110.0" text="邮箱" />
      <Label layoutX="88.0" layoutY="162.0" text="验证码" />
      <TextField fx:id="email" layoutX="179.0" layoutY="104.0" />
      <TextField fx:id="postword" layoutX="179.0" layoutY="156.0" />
      <Button fx:id="submitBtn" layoutX="450.0" layoutY="156.0" mnemonicParsing="false" onAction="#send" text="发送" />
      <Button fx:id="loginBtn" layoutX="259.0" layoutY="221.0" mnemonicParsing="false" onAction="#login" text="登录" />
      <CheckBox fx:id="worker" layoutX="179.0" layoutY="278.0" mnemonicParsing="false" onAction="#chenged" text="员工" />
      <CheckBox fx:id="citizen" layoutX="352.0" layoutY="278.0" mnemonicParsing="false" onAction="#chenged1" text="居民" />
      <ImageView fx:id="image" fitHeight="79.0" fitWidth="364.0" layoutX="118.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../picture/huanyingtu.png" />
         </image></ImageView>
   </children>
</AnchorPane>
